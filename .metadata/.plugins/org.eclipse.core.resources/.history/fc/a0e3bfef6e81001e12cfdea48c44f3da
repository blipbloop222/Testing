package test;
import java.util.*;

public class Test1 {
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner mc = new Scanner(System.in);
        
        while(true){
        System.out.print("Enter an even integer [negative number to quit] : ");
        int even=mc.nextInt();  
        if(even==2){
        System.out.println("Goldbach's Conjecture is not satisfied for the given input.");
    }
        if(even<0){
        break;
}
        for(int num1=even-2;num1>0;num1--){
        int num2=even-num1;
        int count=0;
        for(int i=1;i<=num1;i++){
        if(num1%i==0){
        count++;
}
}
        if(count==2){
        int count1=0;
        for(int a=1;a<=num2;a++){
        if(num2%a==0){
        count1++;
    }
    }
        if(count1==2){
        System.out.println(num1+" + "+num2+" = "+even);
}
}
}
}
}
}


