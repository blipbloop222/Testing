package youtube;
import java.util.*;

public class Me {

	public static void main(String[] args) {
//		 TODO Auto-generated method stub
		int[][] Arr = {{1, 5, 7}, {3, 6, 9}, {5, 3, 8}};
		
		for (int i=0; i<Arr.length-1; i++) {
			for (int j=0; j<=i; j++) {
				Arr[i][j] = Arr[i][j+1];
				if (j>2) {
					i+=1;
				}
			}
		}
		
		for (int k=0; k<Arr.length; k++) {
			for (int l=0; l<Arr[k].length; l++) {
				System.out.print(Arr[k][l] + " ");
			}
			System.out.println();
		}
	}
}

