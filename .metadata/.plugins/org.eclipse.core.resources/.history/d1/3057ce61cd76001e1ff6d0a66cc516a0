package lecture4;

import java.util.Random;

public class lecture4 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Random r = new Random();
		int num = r.nextInt(101);
		for (int count = 0; count <= 5; count++) {
			if (num%2 != 0)
				continue;
			
		
		}
		if (num%2 == 0)
			System.out.println(num);
	}

}