package lecture4;

import java.util.Random;

public class lecture4 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num = 0;
		for (int count = 0; count <= 5; count++) {
			Random r = new Random();
			num = r.nextInt(101);
			if (num%2 != 0)
				continue;
			
		}
		System.out.println(num);
	}

}