package viva2;
import java.util.*;

public class Viva2Q1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		System.out.print("Enter desired number of rows: ");
		int a = sc.nextInt();		
		System.out.print("Enter desired number of columns: ");
		int b = sc.nextInt();
		
		int[][] Arr1 = new int[a][b];
		int[][] Arr2 = new int[a][b];
		
		System.out.println("Enter integer array input in the order from up to down and from left to right. Press Enter after each input.");
		for (int i = 0; i < Arr1.length; i++) {
			for (int j = 0; j < Arr1[i].length; j++) {
				Arr1[i][j] = sc.nextInt();
			}
		}
		
		for (int i = 0; i < Arr1.length; i++) {
			for (int j = 0; j < Arr1[i].length; j++) {
				System.out.println(Arr1[i][j]);
			}
		}
	}

}
