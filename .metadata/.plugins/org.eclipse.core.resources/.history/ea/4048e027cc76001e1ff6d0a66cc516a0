package lecture4;

import java.util.Random;

public class lecture4 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Random r = new Random();
		int sum;
		while (sum<30) {
			int rand = r.nextInt();
			sum += rand;
		if (sum>30)
			break;
		System.out.println(sum);
	}

}